{
  "editor.fontSize": 15,
  "editor.fontFamily": "'Dank Mono'",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 70,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.overviewRulerBorder": false,
  "editor.formatOnSave": false,
  "editor.tabCompletion": "on",
  "editor.tokenColorCustomizations": {
    "comments": "#229977"
  },

  "editor.stickyScroll.enabled": true,

  "editor.bracketPairColorization.enabled": false,

  "explorer.openEditors.visible": 0,

  "files.insertFinalNewline": true,
  "files.autoSave": "off",

  "window.menuBarVisibility": "toggle",
  "window.closeWhenEmpty": false,

  "workbench.colorTheme": "Catppuccin Frapp√©",
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": true,
  "workbench.startupEditor": "none",
  "workbench.editor.showIcons": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.enablePreview": false,

  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "'Dank Mono'",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,

  "peacock.affectStatusBar": true,
  "peacock.favoriteColors": [
    { "name": "Azure Blue", "value": "#007fff" },
    { "name": "JavaScript Yellow", "value": "#f9e64f" },
    { "name": "Gatsby Purple", "value": "#639" },
    { "name": "Vue Green", "value": "#42b883" },
    { "name": "Rust", "value": "#b94700" },
  ],

  "redhat.telemetry.enabled": false,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "vscode-pets.petType": "totoro",
  "vscode-pets.petColor": "purple",
  "vscode-pets.theme": "forest",
  "catppuccin.italicKeywords": false
}
